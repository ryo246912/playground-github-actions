{
  "name": "playground-github-actions",
  "version": "3.0.0",
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "preview": "wrangler pages dev ./dist",
    "build": "astro check && tsc --noEmit && astro build && jampack ./dist",
    "bump:build": "npm run build",
    "bump:deploy": "wrangler pages deploy ./dist --project-name blog",
    "bump": "npm run bump:build && npm run bump:deploy",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --plugin-search-dir=. --check .",
    "lint": "npm run lint:eslint ; npm run lint:prettier",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier --plugin-search-dir=. --list-different --write .",
    "fix": "npm run fix:eslint ; npm run fix:prettier",
    "check": "astro check",
    "sync": "astro sync",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/check": "^0.2.1",
    "@astrojs/rss": "^3.0.0",
    "@resvg/resvg-js": "^2.4.1",
    "astro": "^3.6.4",
    "fuse.js": "^6.6.2",
    "github-slugger": "^2.0.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.10.8",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@astrojs/react": "^3.0.2",
    "@astrojs/sitemap": "^3.0.3",
    "@astrojs/tailwind": "^5.0.0",
    "@divriots/jampack": "^0.34.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/github-slugger": "^1.3.0",
    "@types/react": "^18.2.22",
    "@typescript-eslint/parser": "^6.7.3",
    "astro-eslint-parser": "^0.17.0",
    "eslint": "^8.50.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^0.34.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "wrangler": "^4.19.1"
  }
}
